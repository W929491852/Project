<?xml version="1.0" encoding="UTF-8"?>
<!--约束文件-->
<!DOCTYPE hibernate-mapping
        SYSTEM
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd" >
<hibernate-mapping package="com.qf.domain">
    <class name="User" table="tb_User" >
  		<id name="id">
  			<generator class="native"></generator>
  		</id>
  		<property name="username" length="255"></property>
  		<property name="password" length="20"></property>
  		<property name="gender" length="20"></property>
  		<property name="code" length="255"></property>
  		<property name="flag"></property>
  		<property name="role"></property>
    </class>

</hibernate-mapping>